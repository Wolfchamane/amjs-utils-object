{
    "extension": ["js"],
    "exclude": ["tests/**/*.mjs", "node_modules"],
    "include": ["src/**/*.mjs"],
    "extends": "@istanbuljs/nyc-config-babel",
    "reporter": ["html"],
    "report-dir": "./coverage",
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "check-coverage": true
}
